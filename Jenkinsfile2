@Library('marz-sharedlibs') _
 
pipeline{
  agent any 
  tools {
    maven "maven3.8.6"
  }  
  stages {
    stage('1GetCode'){
      steps{
        sh "echo 'cloning the latest application version' "
        git credentialsId: 'gitHubcredentials2', url: 'https://github.com/marztuma/tesla-app'
      }
    }
   stage ('build'){
       steps{
          common("Build")
           }
   }
   stage("Execute SonarQube Report"){ 
       steps{
          common("SonarQube Report")
            }
    }
    stage("Upload Artifacts Into Nexus"){ 
       steps{
          common("Upload Into Nexus")
           }
    }
  }
}
